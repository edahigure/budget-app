<nav class="navbar navbar-expand-lg navbar-light navbar-custom">
<div class="container ">
    <a class="nav-link navbar-brand " href=<%= root_path %> ><i class="fa-solid fa-arrow-left fa-xl"></i></i></a>
    <div class="main-font">Log in</div>    
</div>
</nav>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="form-group">
  <%= f.email_field :email , placeholder:"email",class:"form-control item"%>
  </div>


  <div class="form-group">
  <%=  f.password_field :password , placeholder:"password",class:"form-control item"%>
  </div>


  <div class="form-group">
    <%= f.submit "Log in" , class: "btn btn-primary"  %>
  </div>

<% end %>

